<mvc:View controllerName="com.shunyu.lqp.fiori-test-boeing.controller.Main" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<Page title="{i18n>TIT_RESERVATION_PAGE_TITLE}"  titleAlignment="Center">
		<content>
			<List>
				<items>
					<CustomListItem type="Active" press="handleNavigation">
						<ObjectHeader icon="sap-icon://appointment" title="R00001" backgroundDesign="Transparent" number="Planned" numberState="Success">
							<attributes>
								<ObjectAttribute text="{i18n>LABEL_START_TIME}"/>
								<ObjectAttribute text="{i18n>LABEL_END_TIME}"/>
							</attributes>
							<statuses>
								<ObjectStatus text="2020-05-02 10:00"/>
								<ObjectStatus text="2020-05-02 22:00"/>
							</statuses>
						</ObjectHeader>
					</CustomListItem>
				</items>
			</List>
		</content>
	</Page>
</mvc:View>